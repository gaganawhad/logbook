  <tr>
    <td><%=h table_entry.title %></td>
    <td><%=h table_entry.description %></td>
    <td><%=h table_entry.solution %></td>
    <td><%=h table_entry.user.name %></td>
    <td><%= link_to table_entry.sha_of_problem, print_github_commit_link(table_entry.github_project.title,  table_entry.sha_of_problem) %></td>
    <td><%= link_to table_entry.sha_of_solution, print_github_commit_link(table_entry.github_project.title, table_entry.sha_of_solution) %></td>
    <td><%= link_to table_entry.github_project.title, print_github_project_link(table_entry.github_project.title) %></td>
    <td><% table_entry.tag_list.sort!.each do |tag| %>
        <%= link_to tag, :action => "label", :controller => "search", :tag=>tag %>
        <% end %>
    </td>
    <td><%=h smart_date_printer table_entry.created_at %></td>
    <td><%=h smart_date_printer table_entry.updated_at %></td>
    <td><%= link_to 'Show', table_entry %></td>
    <td><%= link_to 'Edit', edit_log_book_entry_path(table_entry) %></td>
    <td><%= link_to 'Destroy', table_entry, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>

